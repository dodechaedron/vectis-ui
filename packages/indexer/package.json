{
  "name": "@vectis/indexer",
  "version": "0.0.1",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "subql build",
    "publish": "subql publish -f",
    "dev": "rm -rf ./.data && npm run build && npm run start:docker",
    "prepack": "rm -rf dist && npm build",
    "codegen": "subql codegen -f",
    "start:docker": "docker-compose pull && docker-compose up --remove-orphans"
  },
  "files": [
    "dist",
    "schema.graphql",
    "juno-main.yaml",
    "juno-testnet.yaml"
  ],
  "devDependencies": {
    "@cosmjs/stargate": "^0.28.9",
    "@subql/cli": "^1.9.1",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@subql/types-cosmos": "^0.4.0",
    "@types/node": "^17.0.21",
    "pino": "^7.8.0",
    "ts-proto": "^1.112.1",
    "tslib": "^2.3.1",
    "uuid": "^9.0.0"
  }
}
